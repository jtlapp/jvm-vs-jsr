{
  "name": "deno-fastify",
  "version": "0.1.0",
  "description": "API server for Deno using Fastify and postgres",
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-run --allow-read --allow-write --unstable-worker-options --node-modules-dir src/app.ts"
  },
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 80,
      "singleQuote": true
    }
  },
  "imports": {
    "fastify": "npm:fastify@5.1.0"
  },
  "exports": {
    ".": "./src/app.ts"
  }
}
